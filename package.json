{
	"name": "wow.export",
	"description": "Export Toolkit for World of Warcraft",
	"license": "MIT",
	"version": "0.2.0",
	"type": "module",
	"contributors": [
		"Kruithne <kruithne@gmail.com> (https://kruithne.net/)",
		"Martin Benjamins <marlamin@marlamin.com> (https://wow.tools/)"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Kruithne/wow.export.git"
	},
	"bugs": {
		"url": "https://github.com/Kruithne/wow.export/issues"
	},
	"homepage": "https://github.com/Kruithne/wow.export#readme",
	"scripts": {
		"test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
		"build": "node --no-warnings ./build.js --code --style --assets --framework --shaders",
		"build-debug": "node --no-warnings ./build.js --debug --code --style --assets --framework --shaders",
		"update": "node --no-warnings ./build.js --code",
		"update-debug": "node --no-warnings ./build.js --debug --code --style",
		"update-shaders": "node --no-warnings ./build.js --debug --shaders",
		"update-style": "node --no-warnings ./build.js --debug --style",
		"build-release": "node --no-warnings ./build.js --code --style --assets --shaders --framework --update --package",
		"vue-devtools": "npx vue-devtools"
	},
	"devDependencies": {
		"@kogs/argv": "^1.1.0",
		"@kogs/logger": "^3.3.7",
		"@kogs/utils": "^1.4.9",
		"@types/chrome": "^0.0.210",
		"@types/node": "^18.11.18",
		"@types/nw.js": "^0.13.16",
		"@types/offscreencanvas": "^2019.7.0",
		"@types/three": "^0.148.1",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"@vue/devtools": "^6.5.0",
		"eslint": "^8.32.0",
		"eslint-plugin-jest": "^27.2.1",
		"eslint-plugin-vue": "^9.9.0",
		"jest": "^29.3.1",
		"jszip": "^3.10.1",
		"pkg": "^5.8.0",
		"tailwindcss": "^3.2.7",
		"three": "^0.149.0",
		"ts-loader": "^9.4.2",
		"uuid": "^9.0.0",
		"vue": "^3.2.45",
		"vue-loader": "^17.0.1",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	}
}
