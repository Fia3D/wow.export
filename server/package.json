{
  "name": "wowexport.net",
  "module": "index.ts",
  "devDependencies": {
    "bun-types": "^0.5.0"
  },
  "spooder": {
    "autoRestart": 1e4,
    "run": "bun run index.ts",
    "update": [
      "git reset --hard",
      "git clean -fd",
      "git pull origin main",
      "bun install"
    ],
    "canary": {
      "account": "kruithne",
      "repository": "kruithne/wow.export",
      "labels": ["⚠️ canary-report"],
      "throttle": 86400
    }
  },
  "type": "module",
  "dependencies": {
    "spooder": "^3.2.1"
  }
}